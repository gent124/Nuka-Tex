<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css"
        integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

</head>

<body>


    <% productsList.forEach(products=> {%>

        <p>
            <% products.productId%>
        </p>


        <%})%>


            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Product ID</th>
                        <th scope="col">Product Name</th>
                        <th scope="col">Product Amount Bought</th>
                        <th scope="col">Product Price per meter</th>
                        <th scope="col">Product Amount Sold</th>
                        <th scope="col">Product Amount Remaining</th>
                    </tr>
                </thead>
                <tbody>
                    <%productsList.forEach(products=> {%>
                        <tr>
                            <th scope="row">
                                <%= products.productId %>
                            </th>
                            <td>
                                <%= products.productName %>
                            </td>

                            <td>
                                <%= products.productAmountBought%>
                            </td>
                            <td>
                                <%= products.productPricePerMeter%>
                            </td>
                            <td>
                                <%= products.productAmountSold%>
                            </td>
                            <td>
                                <%= products.productAmountRemaining%>
                            </td>
                        </tr>
                        <%})%>
                            <!-- <% productsList.forEach(product=> {%>
                                <tr>
                                    <th scope="row">
                                        <% product._id %>
                                    </th>
                                    
                                </tr>

                                <%})%> -->



                </tbody>
            </table>


</body>

</html>